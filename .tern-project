{
  "ecmaVersion": 6,
  "libs": [ ],
  "loadEagerly": [
    "routes/**/*.js",
    "views/**/*.ejs",
    "node_modules/**/*.js",
    "bin/www"
  ],
  "plugins": {
    "complete_strings": {
      "maxLength": 15
    },
    "node": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    },
    "node_resolve": {},
    "doc_comment": {
      "fullDocs": true,
      "strong": true
    },
    "modules": {
      "dontLoad": "",
      "load": "",
      "modules": ""
    }
  }
}
